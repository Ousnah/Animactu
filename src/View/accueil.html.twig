</head>
<body>
    {% include "header.html.twig"%}
      <main>
       <div class="content">

    <h1>Articles</h1>
    <section class="discover" id="discover">
    
    {% for article in articles %}
        <article>
            <h3>{{ article.image}}</h3>
            <h3>{{ article.title}}</h3>
            <h3>{{ article.date}}</h3>
        </article>

    {% endfor %}
    </div>
    

    {% block content %}{% endblock %}
    </div>
    
  </main>

{% include "footer.html.twig"%}
</body>
</html>